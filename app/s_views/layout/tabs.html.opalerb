<ul class="tabs" data-tab>
  <% self.digests.each_index do |i| %>
      <% digest = self.digests[i] %>

      <% active = (i == self.active_digest_index ) %>
      <% active_class= (active ? 'active' : '') %>

      <li class="tab-title <%= active_class %>">
        <a href="#digest_<%= i %>" id="open_digest_<%= i %>"><%= digest.key.underscore.split('_')[0].capitalize %></a></li>
  <% end %>
</ul>

